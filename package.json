{
  "name": "products-grid",
  "private": true,
  "version": "0.0.1",
  "description": "Build a grid of product items and ads",
  "main": "server/index.js",
  "scripts": {
    "start": "concurrently \"PORT=5000 react-scripts start\" \"json-server server/index.js\" \"--routes server/routes.json\" \"--middlewares server/handle-delay.js\" \"server/handle-ads.js\"",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "concurrently \"PORT=5000 react-scripts start\" \"json-server server/index.js --routes server/routes.json --middlewares server/handle-delay.js server/handle-ads.js\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:3000",
  "author": "",
  "license": "",
  "dependencies": {
    "@material-ui/core": "^4.4.2",
    "concurrently": "^4.1.2",
    "cool-ascii-faces": "^1.3.4",
    "json-server": "^0.9.6",
    "node-sass": "^4.14.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-redux": "^7.1.1",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "react-scripts": "^3.4.1",
    "react-scripts-sass": "^1.1.3",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "uuid": "^3.3.3"
  }
}
